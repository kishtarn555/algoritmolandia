    <script>
        function showNavBar() {
          document.getElementById('fade-screen').style.display='block';
          document.getElementById('side-bar').style.left='0';
        }
        function hideNavbar() {
          document.getElementById('fade-screen').style.display='none';
          document.getElementById('side-bar').style.left='-260px';
        }
        $( document ).on( 'keydown', function ( e ) {
        if ( e.keyCode === 27 && document.getElementById('side-bar').style.left=="0px") { // ESC
            
            hideNavbar();
        }
});
    </script>
